{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:recommended",
		"tslint-config-prettier"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**"
		]
	},
	"rules": {
		"indent": [
			true,
			"tabs",
			2
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"no-consecutive-blank-lines": true,
		"no-console": false,
		"member-access": [
			true,
			"no-public"
		],
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"quotemark": [
			true,
			"double"
		],
		"semicolon": [
			true,
			"never"
		],
		"trailing-comma": false
	}
}
